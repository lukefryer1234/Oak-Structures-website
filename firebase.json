{
  "hosting": {
    "source": "hosting",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "memory": "1024MB"
    }
  },
  "functions": {
    "source": "hosting",
    "runtime": "nodejs20",
    "memory": "1024MB",
    "region": "us-central1"
  }
}
